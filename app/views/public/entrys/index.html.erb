<h1>自分のエントリー</h1>

<%= month_calendar events: @entrys do |date, entrys| %>
  <%= date.day %>

  <% entrys.each do |entry| %>
    <div>
      <%= link_to entry.event.name,entry_path(entry.id)  %>
    </div>
  <% end %>

<% end %>
<% @entrys.each do |entry| %>
<p><%= link_to entry.event.name, event_path(entry.event_id) %><p>
  <p>順位</p>
  <p><%= entry.rank %><p>
  <p>タイム</p>
  <p><%= entry.time %><p>


  ---------------------------------------------------------
<% end %>

